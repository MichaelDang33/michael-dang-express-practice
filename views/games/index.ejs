<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>First Express</title>
</head>
<body>
    <h1>All Games</h1>
    <ul>
        <% games.forEach(function (g) { %>
            <li>
                <%= g.game %>
                  -
                <%= g.done ? "done" : "not done" %>
            </li>
        <% }); %>
    </ul>
</body>
</html>